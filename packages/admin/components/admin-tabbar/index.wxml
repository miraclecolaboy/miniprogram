
<view class="admin-tabbar__spacer"></view>

<view class="admin-tabbar">
  <view
    wx:for="{{tabs}}"
    wx:key="key"
    class="admin-tabbar__item {{active === item.key ? 'is-active' : ''}}"
    data-key="{{item.key}}"
    bindtap="onTap"
  >
    <text class="admin-tabbar__text">{{item.text}}</text>
  </view>
</view>
